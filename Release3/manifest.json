{
"manifest_version":2,
"name":"CoronaVirus Tracker",
"description":"Daily tracker for COVID19 patients worldwide",
"version":"1.0.0",
"icons":{ "96":"icons8-coronavirus-96.png",
          "50":"icons8-coronavirus-50.png"
},

"chrome_url_overrides":{
    "newtab":"popup.html"
},
"background":{
    "scripts":["background.js"],
    "persistent":false
},
"browser_action": { 
    "default_icon":"icons8-coronavirus-50.png"
},

"permissions":["<all_urls>","tabs"]   

}